
@model ActiveMenu.ViewModel.CreateItemVM
@{
    ViewBag.Title = "createItem";
    Layout = "~/Views/Shared/_managementLayout.cshtml";
}
@{
    string title = "";
    string desc = "";
    string inst = "";
    string cal = "";
    string carb = "";
    string fat = "";
    string prot = "";
    string price = "";
    string time = "";
    string itemID = "";
    if (Model.item != null)
    {
        title = Model.item.title;
        desc = Model.item.desctiption;
        inst = Model.item.Instruction;
        cal = Model.item.calory.ToString();
        carb = Model.item.Carbohydrates.ToString();
        fat = Model.item.Fats.ToString();
        prot = Model.item.Proteins.ToString();
        price = Model.item.price.ToString();
        time = Model.item.time.ToString();
        itemID = Model.item.itemID.ToString();


    }
}
