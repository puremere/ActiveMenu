@section scripts{
    <script>
        var idtoremove = "";
        var myremovePrentClicked = function (id) {
           
            idtoremove = id;
           
            $("#confirmDelete").modal('toggle');
        }
        //$(".myrmoveparent").click(function () {

        //    var id = $(this).attr('id');
        //    idtoremove = id;
        //    $("#confirmDelete").modal('toggle');

        //})
        $("#RemoveIngButton").click(function () {
            //alert(idtoremove);
            $.ajax({
                url: "/Admin/removeIngredientTo",
                data: {
                    mid: idtoremove
                },
                success: function (data) {
                    $("#" + idtoremove).remove();
                    $("#confirmDelete").modal('toggle');
                }
            })
        })
        $("#addOrder").click(function () {
            var catInput = $("#catItem").val();
            var ingredientInput = $("#ingredientInput").val();
            var descriptionInput = $("#Description").val();
            var instruction = $("#instruction").val();
            var priceInput = $("#Price").val();
            var titleInput = $("#uname").val();

